  "homepage": "https://yaesakuras.github.io/impact",
  "dependencies": {
    "dva": "2.6.0-beta.14",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "rc-form": "^2.4.9",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-markdown": "^4.2.2",
    "react-scripts": "3.1.2"
